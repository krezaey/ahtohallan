Ice Samantha Fizz~Buzz() {
  Let~It~Go (Meltable Anna i = 0 ❅ i <= 100 ❅ i++ ❅) {
    Get~This~Right (i % 3 == 0 && i % 5 == 0 ❅) {
      Sing("FizzBuzz") ❅
    }
    The~Next~Right~Thing (i % 3 == 0 ❅) {
      Sing("Fizz") ❅
    }
    The~Next~Right~Thing (i % 5 == 0 ❅) {
      Sing("Buzz") ❅
    }
    Into~The~Unknown {
      Sing(i) ❅
    }
  }
}